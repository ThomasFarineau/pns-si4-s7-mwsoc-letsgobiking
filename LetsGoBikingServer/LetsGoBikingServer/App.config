<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<appSettings>
		<add key="aspnet:UseTaskFriendlySynchronizationContext" value="false"/>
	</appSettings>
	<system.web>
		<compilation debug="true"/>
	</system.web>
    <system.serviceModel>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/LetsGoBikingServer/ProxyService/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IProxyService1"
                contract="ProxyService.IProxyService" name="BasicHttpBinding_IProxyService" />
        </client>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IProxyService" />
                <binding name="BasicHttpBinding_IProxyService1" />
            </basicHttpBinding>
        </bindings>
       <services>
            <service name="LetsGoBikingServer.LetsGoBiking">
                <endpoint address="" binding="basicHttpBinding" contract="LetsGoBikingServer.ILetsGoBiking">
                    <identity>
                        <dns value="localhost"/>
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8733/Design_Time_Addresses/LetsGoBikingServer/ILetsGoBiking/"/>
                    </baseAddresses>
                </host>
            </service>
        </services>
        <behaviors>
            <serviceBehaviors>
                <behavior>
                    <serviceMetadata httpGetEnabled="True" httpsGetEnabled="True"/>
                    <serviceDebug includeExceptionDetailInFaults="True"/>
                </behavior>
            </serviceBehaviors>
        </behaviors>
    </system.serviceModel>
</configuration>